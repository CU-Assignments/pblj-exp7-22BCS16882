CREATE DATABASE EMP;
SHOW DATABASES;
USE EMP;

CREATE TABLE EMPLOYEE (
    EmpID INT PRIMARY KEY,
    Name VARCHAR(100),
    Salary DECIMAL(10, 2)
);

